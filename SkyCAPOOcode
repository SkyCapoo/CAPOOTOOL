label(returnhere)
alloc(bulletID,4)
registersymbol(bulletID)
bulletID:
dd #15003
newmem:
mov esi,[bulletID]//子弹ID
mov [eax+88],esi
mov esi,[eax+88]
jmp returnhere
changebutten:
jmp newmem
nop
nop
nop
returnhere:
]=])
end
sleep(50)
writeInteger("bulletID",getProperty(UDF2_CEEdit20,"text"))
writeInteger("[[[libSandboxEngine.g_pPlayerCtrl]+5B8]+0]+88",getProperty(UDF2_CEEdit20,"text"))
end

