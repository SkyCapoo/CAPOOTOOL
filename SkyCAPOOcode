label(code)
label(return)
newmem:
code:
  mov eax,#945804388
  xor eax,385FD4AC
  jmp return
INJECT:
  jmp newmem
  nop 3
return:
registersymbol(INJECT)
]=])
else
autoAssemble([=[{$asm}
INJECT:
  db 8B 41 34 35 AC D4 5F 38
unregistersymbol(INJECT)
dealloc(newmem)
]=])
end
end

